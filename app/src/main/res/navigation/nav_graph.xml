<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/TransactionFragment"
        android:name="com.isrbet.budgetsbyisrbet.TransactionFragment"
        android:label="@string/expenditures"
        tools:layout="@layout/fragment_transaction">
        <argument
            android:name="transactionID"
            app:argType="string"
            android:defaultValue='' />
        <action
            android:id="@+id/action_TransactionFragment_to_TransferFragment"
            app:destination="@id/TransferFragment" />
    </fragment>
    <fragment
        android:id="@+id/ScheduledPaymentFragment"
        android:name="com.isrbet.budgetsbyisrbet.ScheduledPaymentFragment"
        android:label="@string/scheduled_payments"
        tools:layout="@layout/fragment_scheduled_payment">
        <action
            android:id="@+id/action_ScheduledPaymentFragment_to_LoanFragment"
            app:destination="@id/LoanFragment" />
    </fragment>
    <fragment
        android:id="@+id/SettingsFragment"
        android:name="com.isrbet.budgetsbyisrbet.SettingsFragment"
        android:label="@string/settings"
        tools:layout="@layout/fragment_settings" />
    <fragment
        android:id="@+id/TransactionViewAllFragment"
        android:name="com.isrbet.budgetsbyisrbet.TransactionViewAllFragment"
        android:label="View Transactions"
        tools:layout="@layout/fragment_transaction_view_all" >
        <argument
            android:name="accountingFlag"
            app:argType="string"
            android:defaultValue='' />
        <argument
            android:name="categoryID"
            app:argType="string"
            android:defaultValue='' />
        <action
            android:id="@+id/action_TransactionViewAllFragment_to_TransactionFragment"
            app:destination="@id/TransactionFragment" />
        <action
            android:id="@+id/action_TransactionViewAllFragment_to_TransferFragment"
            app:destination="@id/TransferFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.isrbet.budgetsbyisrbet.HomeFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_TransactionViewAllFragment"
            app:destination="@id/TransactionViewAllFragment" />
    </fragment>
    <fragment
        android:id="@+id/DashboardFragment"
        android:name="com.isrbet.budgetsbyisrbet.DashboardFragment"
        android:label="@string/dashboard"
        tools:layout="@layout/fragment_dashboard" >
        <action
            android:id="@+id/action_DashboardFragment_to_TransactionViewAllFragment"
            app:destination="@id/TransactionViewAllFragment" />
    </fragment>
    <fragment
        android:id="@+id/SettingsTabsFragment"
        android:name="com.isrbet.budgetsbyisrbet.SettingsTabsFragment"
        android:label="@string/settings"
        tools:layout="@layout/fragment_settings_tabs">
        <action
            android:id="@+id/action_SettingsTabsFragment_to_BudgetViewAllFragment"
            app:destination="@id/BudgetViewAllFragment" />
        <action
            android:id="@+id/action_SettingsTabFragment_to_BudgetFragment"
            app:destination="@id/BudgetFragment" />
        <action
            android:id="@+id/action_SettingsTabFragment_to_TransactionViewAllFragment"
            app:destination="@id/TransactionViewAllFragment" />
        <action
            android:id="@+id/action_SettingsTabFragment_to_LoanFragment"
            app:destination="@id/LoanFragment" />
    </fragment>
    <fragment
        android:id="@+id/AdminFragment"
        android:name="com.isrbet.budgetsbyisrbet.AdminFragment"
        android:label="@string/admin"
        tools:layout="@layout/fragment_admin" />
    <fragment
        android:id="@+id/CategoryFragment"
        android:name="com.isrbet.budgetsbyisrbet.CategoryFragment"
        android:label="@string/categories"
        tools:layout="@layout/fragment_category">
    <action
        android:id="@+id/action_CategoryFragment_to_BudgetViewAllFragment"
        app:destination="@id/BudgetViewAllFragment" />
    <action
        android:id="@+id/action_CategoryFragment_to_TransactionViewAllFragment"
        app:destination="@id/TransactionViewAllFragment" />
    <action
        android:id="@+id/action_CategoryFragment_to_BudgetFragment"
        app:destination="@id/BudgetFragment"/>
    </fragment>
    <fragment
        android:id="@+id/AccountingFragment"
        android:name="com.isrbet.budgetsbyisrbet.AccountingFragment"
        android:label="@string/accounting">
        <action
            android:id="@+id/action_AccountingFragment_to_TransactionViewAllFragment"
            app:destination="@id/TransactionViewAllFragment" />
    </fragment>
    <fragment
        android:id="@+id/TransferFragment"
        android:name="com.isrbet.budgetsbyisrbet.TransferFragment"
        android:label="@string/transfer">
        <argument
            android:name="mode"
            app:argType="integer"
            android:defaultValue='0' />
        <argument
            android:name="transactionID"
            app:argType="string"
            android:defaultValue='' />
    </fragment>
    <fragment
        android:id="@+id/HelpFragment"
        android:name="com.isrbet.budgetsbyisrbet.HelpFragment"
        android:label="@string/help"
        tools:layout="@layout/fragment_help" />
    <fragment
        android:id="@+id/BudgetFragment"
        android:name="com.isrbet.budgetsbyisrbet.BudgetFragment"
        android:label="fragment_budget"
        tools:layout="@layout/fragment_budget" >
        <argument
            android:name="categoryID"
            app:argType="string"
            android:defaultValue="" />
    </fragment>
    <fragment
        android:id="@+id/BudgetViewAllFragment"
        android:name="com.isrbet.budgetsbyisrbet.BudgetViewAllFragment"
        android:label="fragment_budget_view_all"
        tools:layout="@layout/fragment_budget_view_all" >
        <action
            android:id="@+id/action_BudgetViewAllFragment_to_BudgetFragment"
            app:destination="@id/BudgetFragment" >
        </action>
        <argument
            android:name="categoryID"
            app:argType="string"
            android:defaultValue="" />
        <argument
            android:name="year"
            app:argType="string"
            android:defaultValue="" />
        <argument
            android:name="month"
            app:argType="string"
            android:defaultValue="" />
    </fragment>
    <fragment
        android:id="@+id/TrackerFragment"
        android:name="com.isrbet.budgetsbyisrbet.TrackerFragment"
        android:label="Tracker"
        tools:layout="@layout/fragment_tracker">
        <action
            android:id="@+id/action_TrackerFragment_to_TransactionViewAllFragment"
            app:destination="@id/TransactionViewAllFragment" />
        <action
            android:id="@+id/action_TrackerFragment_to_BudgetViewAllFragment"
            app:destination="@id/BudgetViewAllFragment" />
        <argument
            android:name="type"
            app:argType="string"
            android:defaultValue="" />
    </fragment>
    <fragment
        android:id="@+id/ViewTranslationsFragment"
        android:name="com.isrbet.budgetsbyisrbet.TranslationViewAllFragment"
        android:label="Translations"
        tools:layout="@layout/fragment_translation_view_all" />
    <fragment
        android:id="@+id/CategoryDetailsFragment"
        android:name="com.isrbet.budgetsbyisrbet.CategoryDetailsFragment"
        android:label="CategoryDetails"
        tools:layout="@layout/fragment_category_details" />
    <fragment
        android:id="@+id/LoanFragment"
        android:name="com.isrbet.budgetsbyisrbet.LoanFragment"
        android:label="loans"
        tools:layout="@layout/fragment_loan" >
        <argument
            android:name="loanID"
            app:argType="string"
            android:defaultValue="" />
    </fragment>
    <fragment
        android:id="@+id/RetirementFragment"
        android:name="com.isrbet.budgetsbyisrbet.RetirementFragment"
        android:label="@string/Retirement"
        tools:layout="@layout/fragment_retirement">
        <action
            android:id="@+id/action_RetirementFragment_to_RetirementDetailsFragment"
            app:destination="@id/RetirementDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/RetirementDetailsFragment"
        android:name="com.isrbet.budgetsbyisrbet.RetirementDetailsFragment"
        android:label="@string/RetirementDetails"
        tools:layout="@layout/fragment_retirement_details">
        <argument
            android:name="scenarioName"
            app:argType="string"
            android:defaultValue=''/>
        <argument
            android:name="userID"
            app:argType="integer"
            android:defaultValue='0'/>
    </fragment>
    <fragment
        android:id="@+id/DashboardTabsFragment"
        android:name="com.isrbet.budgetsbyisrbet.DashboardTabsFragment"
        android:label="fragment_dashboard_tabs"
        tools:layout="@layout/fragment_dashboard_tabs">
        <action
            android:id="@+id/action_DashboardTabsFragment_to_BudgetViewAllFragment"
            app:destination="@id/BudgetViewAllFragment" />
    </fragment>
    <fragment
        android:id="@+id/YearOverYearFragment"
        android:name="com.isrbet.budgetsbyisrbet.YearOverYearFragment"
        android:label="YearOverYearFragment"
        tools:layout="@layout/fragment_year_over_year" />
    <fragment
        android:id="@+id/TrackerTabsFragment"
        android:name="com.isrbet.budgetsbyisrbet.TrackerTabsFragment"
        android:label="TrackerTabsFragment"
        tools:layout="@layout/fragment_tracker_tabs" />
</navigation>